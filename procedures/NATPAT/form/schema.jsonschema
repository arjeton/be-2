        "feesSelection": {
          "title": "i18n(fees.selection)",
          "type": "object",
          "properties": {
            "selectedFees": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "code": {
                    "enum": [
                      "001",
                      "0011",
                      "002",
                      "003"
                    ]
                  },
                  "price": {
                    "$ref": "/procedures/common/form/field-types.jsonschema/definitions/positiveNumber"
                  },
                  "discount": {
                    "$ref": "/procedures/common/form/field-types.jsonschema/definitions/percentageNumber"
                  },
                  "quantity": {
                    "$ref": "/procedures/common/form/field-types.jsonschema/definitions/positiveNumber",
                    "minimum": 1
                  },
                  "subtotal": {
                    "$ref": "/procedures/common/form/field-types.jsonschema/definitions/positiveNumber"
                  }
                }
              }
            },
            "total": {
              "$ref": "/procedures/common/form/field-types.jsonschema/definitions/positiveNumber"
            },
            "currency": {
              "enum": [
                "EUR"
              ]
            }
          }
        }
      },
      "required": [
        "methodOfPayment"
      ],
      "dependencies": {
        "methodOfPayment": {
          "oneOf": [
            {
              "properties": {
                "methodOfPayment": {
                  "enum": [
                    "payOnline"
                  ]
                }
