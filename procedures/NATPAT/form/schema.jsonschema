  "$sid": "NATPAT/form/schema.jsonschema",
  "type": "object",
  "properties": {
    "basicFilingInfo": {
      "type": "object",
      "title": "i18n(basic-filing.info)",
      "description": "i18n(basic_filing_info.description)",
      "properties": {
        "userReference": {
          "title": "i18n(basic-filing.user-reference)",
          "type": [
            "string",
            "null"
          ],
          "maxLength": 25,
          "pattern": "^[\\d\\w\\s\\\\.,<>=_\\-()/'\"\\\\À-ž\u0600-\u06FF]*$"
        },
        "caseId": {
          "type": "string",
          "title": "i18n(basic-filing.caseId)"
        },
        "titleOfInvention": {
          "title": "i18n(basic-filing.title-of-invention-first-language) i18n(optional)",
          "type": ["string", "null"],
          "maxLength": 512
        },
        "applicationType": {
          "$ref": "#/definitions/applicationType"
        },
        "typeOfSearchReportContainer": {
          "title": "i18n(basic-filing.type-of-search-report)",
          "type": [
            "string",
            "null"
          ],
          "enum": [
            "priorSearch",
            "nationalSearch",
            "internationalSearch",
            "sentLater"
          ],
          "enumNames": [
            "i18n(basic-filing.prior-search-report)",
            "i18n(basic-filing.national-search-report)",
            "i18n(basic-filing.international-search-report)",
            "i18n(basic-filing.search-report-send-later)"
          ],
          "default": "priorSearch"
        },
        "fastTrack": {
          "title": "i18n(basic-filing.fastTrack)",
          "type": "boolean"
        }
      },
      "dependencies": {
        "applicationType": {
          "oneOf": [
            {
              "properties": {
                "applicationType": {
                  "enum": ["NATIONAL_PATENT"]
                }
              }
            },
            {
              "properties": {
                "applicationType": {
                  "enum": ["DIVISIONAL"]
                },
                "divisionalNumber": {
                  "title": "i18n(basic-filing.divisional-number)",
                  "pattern": "^[0-9]{0,11}$",
                  "type": "string",
                  "maxLength": 11
                },
                "divisionalFilingDate": {
                  "title": "i18n(basic-filing.divisional-filing-date)",
                  "type": [
                    "string",
                    "null"
                  ]
                }
              },
              "required": ["divisionalNumber", "divisionalFilingDate"]
            }
          ]
        }
      },
      "required": [
        "userReference",
        "typeOfSearchReportContainer",
        "applicationType"
      ]
    },
    "parties": {
      "title": "Parties",
      "type": "object",
      "properties": {
